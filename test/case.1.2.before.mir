constants:       []
machineFunctionInfo:
  varArgsFrameIndex: 0
  varArgsSaveSize: 0
body:             |
  bb.0.entry:
    successors: %bb.1(0x40000000), %bb.2(0x40000000)
    liveins: $x10, $x11
  
    %4:gpr = COPY $x11
    %3:gpr = COPY $x10
    BGE %4, %3, %bb.2
    PseudoBR %bb.1
  
  bb.1.if.then:
    successors: %bb.3(0x80000000)
  
    %5:gpr = LUI target-flags(riscv-hi) @p
    %6:gpr = LW %5, target-flags(riscv-lo) @p :: (dereferenceable load (s32) from @p, !tbaa !6)
    %7:gpr = persistent ADD %6, %4
    %8:gpr = LBU %7, 2 :: (load (s8) from %ir.arrayidx, !tbaa !11)
    %9:gpr = nuw nsw ADDI %8, 10
    PseudoBR %bb.3
  
  bb.2.if.else:
    successors: %bb.3(0x80000000)
  
    %9:gpr = nsw ADDI %3, 13
  
  bb.3.if.end:
    $x10 = COPY %9
    PseudoRET implicit $x10

