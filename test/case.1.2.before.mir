---
name: case.1.2
tracksRegLiveness: true
liveins:
  - { reg: '$x10', virtual-reg: '%1' }
  - { reg: '$x11', virtual-reg: '%2' }
body: |
  bb.0:
    successors: %bb.1, %bb.2
    liveins: $x10, $x11
  
    %1:gpr = COPY $x10
    %2:gpr = COPY $x11
    BGE %1, %2, %bb.2
  
  bb.1:
    successors: %bb.3

    %3:gpr = persistent ADD %1, %2
    PseudoBR %bb.3
  
  bb.2:
    successors: %bb.3

    %4:gpr = ADDI %1, 0
  
  bb.3:
    $x10 = COPY %1
    PseudoRET implicit $x10
...
